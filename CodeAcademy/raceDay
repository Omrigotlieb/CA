let raceNumber = Math.floor(Math.random() * 1000);

let earlyRunner = false;
let age = 10;

if (earlyRunner * age > 18) {
  raceNumber += 1000;
}


if(age > 18) {
  if(earlyRunner) {
    console.log('Race at 09:30, race number:' + raceNumber);
  } else {
    console.log('Late adults run at 11:00 am, race number:' + raceNumber);
  }
} else {
  console.log('Youth registrants run at 12:30 pm (regardless of registration)')
}
